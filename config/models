Usuario json
    tipoUsuario TipoUsuarioId Update
    nombre Text Maybe Update
    fechaNacimiento Day default=CURRENT_DATE Maybe Update
    genero Text Maybe Update
    identificacion Text Maybe Update
    telefono Text Maybe Update
    email Text Maybe Update
    rh Text Maybe Update
    fechaRegistro Day default=CURRENT_DATE Maybe
    deriving Show

PruebasFisicas json
    idUsuario UsuarioId
    peso Int Update
